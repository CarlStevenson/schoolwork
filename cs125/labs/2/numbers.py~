# Purpose: To try calculations in Python

def main():
    num1 = int(input("Choose a number:"))
    num2 = num1 + 3
    num2 = (num2 * 2) - 3
    num2 = num2 - (num1 * 2)
    num2 = num2 + 3
    print("The result is", num2)

main()
 
               
